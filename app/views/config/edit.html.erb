<h1>設定変更</h1>
<div class="form-area">
  <%= form_with model: @user, url: dashboard_config_edit_patch_path, method: :patch, local: true do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-text" %>
    <%= f.label :email %>
    <%= f.email_field :email, class: "form-text" %>
    <%= f.label :password %>
    <%= f.password_field :password, class: "form-text" %>
    <div class="form-submit">
      <%= f.submit "更新する" %>
    </div>
  <% end %>
